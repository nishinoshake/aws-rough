{
  "name": "aws-rough",
  "version": "1.0.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/noplan1989/aws-rough"
  },
  "scripts": {
    "dev": "nuxt",
    "serve": "http-server ./dist -p 8888",
    "analyze": "nuxt build --analyze",
    "generate:test": "cross-env AWSROUGH_GENERATE=e2e nuxt generate",
    "generate:production": "cross-env AWSROUGH_GENERATE=production nuxt generate",
    "jest": "jest",
    "test": "npm run test:unit && npm run test:e2e",
    "test:unit": "jest -c jest.config.unit.js test/unit",
    "test:e2e": "jest -c jest.config.e2e.js test/e2e",
    "test:coverage": "jest --coverage -c jest.config.unit.js test/unit",
    "format": "eslint --fix --ext .js,.vue --ignore-path .gitignore .",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,vue}": [
      "eslint --fix --ext .js,.vue --ignore-path .gitignore .",
      "git add"
    ]
  },
  "dependencies": {
    "@nuxtjs/dotenv": "^1.3.0",
    "@nuxtjs/google-adsense": "^1.1.3",
    "@nuxtjs/google-analytics": "^2.0.2",
    "@nuxtjs/pwa": "^2.6.0",
    "animejs": "^2.2.0",
    "axios": "^0.18.1",
    "chart.js": "^2.7.2",
    "clipboard": "^2.0.0",
    "color": "^3.0.0",
    "fastclick": "^1.0.6",
    "jssha": "^2.3.1",
    "lodash": "^4.17.15",
    "nuxt": "^2.3.4",
    "sweet-scroll": "^3.0.1",
    "url-parse": "^1.4.3",
    "vue-chartjs": "^3.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@vue/test-utils": "^1.0.0-beta.28",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.8.0",
    "cross-env": "^5.2.0",
    "eslint": "^4.18.2",
    "eslint-config-prettier": "^3.1.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-standard": "^3.0.1",
    "eslint-plugin-vue": "^4.3.0",
    "http-server": "^0.11.1",
    "husky": "^0.14.3",
    "jest": "^24.8.0",
    "jest-puppeteer": "^4.1.1",
    "lint-staged": "^7.3.0",
    "node-sass": "^4.11.0",
    "prettier": "^1.14.3",
    "puppeteer": "^1.15.0",
    "regenerator-runtime": "^0.11.1",
    "sass-loader": "^7.1.0",
    "vue-svg-loader": "^0.11.0"
  }
}
